<article class="task" id="<%= dom_id task %>">

  <h2 class="task__title"><%= task.title %></h2>

  <%= link_to "Edit", edit_project_task_path(task.project, task) %>

  <p class="task__user-email"><%= task.assigned_user&.email || "unknown" %></p>

  <p class="task__story-points"><%= task.story_points %></p>

</article>
